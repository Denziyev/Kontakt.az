                                        if (data.indexOf('notavailable') > -1 || data.indexOf('yoxdur') > -1) {
                                            jQuery('.checkout_comment').html(data);
                                            jQuery('.checkout_comment').show();
                                        } else if (data.indexOf('~success-checkout-message~') > -1) {
                                            datascom = data.split("~success-checkout-message~")[1].split("~");
                                            datas = datascom[0];
                                            datasid = datascom[1].replace("</span>", "");

                                            if (data.indexOf('gotoonlinepayment') > -1) {
                                                jQuery(".checkout-row").hide();
