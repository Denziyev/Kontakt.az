                        jQuery.validator.addMethod("greaterThanZero", function (value, element) {
                            alert(parseFloat(value));
                            return this.optional(element) || (parseFloat(value) > 0);
                        }, "* Amount must be greater than zero");

                        jQuery("#checkout_form").validate({
